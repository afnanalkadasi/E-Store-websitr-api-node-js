    <section class="product_section container-fluid mb-5 ">
        <div class="row border-top ">
            <div class="col-lg-3 d-none d-lg-block ">
                <a class="btn shadow-none d-flex align-items-center justify-content-between  text-white w-100 " style="background: #8019c8;" data-toggle="collapse" href="#navbar-vertical" style="height: 65px; margin-top: -1px; padding: 0 30px;">
                    <h3 class="m-0 ">Categories</h3>
                    <i class="fa fa-angle-down text-dark"></i>
                </a>
