                    <div class="navbar-nav w-100 overflow-hidden " style="height: 515px">
                      
                        <% products.forEach(product => { %>
                        <li class="nav-item ">
                            <a class="nav-link" href="/products/category/<%= product.category%>" > <h5><%= product.category%></h5></a>
                        </li>
                        <%})%>
                    </div>
                </nav>
            </div>
           
          

  
    <div class="product_container col-lg-9 product">
    <% for( let index = 0; index < 4 ; index++ ) { %>
      <div class="box">
        <div class="box-content">
          <div class="img-box">
              <a href="/products/<%= products[index].id %> ">
            <img  src="<%= products[index].thumbnail %> " alt="">
            </a>
          </div>
          <div class="detail-box">
            <div class="text">
              <h6 >
